{
  "name": "@callstack/repack",
  "version": "2.4.0",
  "description": "A Webpack-based toolkit to build your React Native application with full support of Webpack ecosystem.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "react-native": "",
  "files": [
    "dist",
    "first-party",
    "android",
    "ios",
    "commands.js",
    "client.js",
    "assets-loader.js",
    "callstack-repack.podspec",
    "!android/build",
    "!ios/build"
  ],
  "homepage": "https://github.com/callstack/repack",
  "repository": "github:callstack/repack",
  "keywords": [
    "react-native",
    "react native",
    "webpack",
    "bundle",
    "bundling",
    "metro"
  ],
  "author": "zamotany <zamotany.oss@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "yarn build",
    "clean": "del dist",
    "build:js": "babel src --out-dir dist --extensions \".js,.ts\" --source-maps --ignore \"**/__tests__/**\"",
    "build:js:watch": "yarn build:js --watch",
    "typecheck": "tsc",
    "build:ts": "tsc --build tsconfig.release.json",
    "build:ts:watch": "yarn build:ts --watch --preserveWatchOutput",
    "build": "yarn clean && yarn build:js && yarn build:ts",
    "lint": "eslint --ext \".js,.ts\" commands.js client.js src",
    "test": "jest",
    "release": "release-it"
  },
  "peerDependencies": {
    "@react-native-community/cli": "*",
    "@react-native-community/cli-types": "*",
    "react-native": ">=0.63",
    "webpack": "5.x"
  },
  "dependencies": {
    "@babel/code-frame": "^7.12.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "colorette": "^1.2.2",
    "dedent": "^0.7.0",
    "escape-string-regexp": "^4.0.0",
    "execa": "^5.0.0",
    "fastify": "^3.12.0",
    "fastify-express": "^0.3.2",
    "fastify-graceful-shutdown": "^3.1.0",
    "fastify-reply-from": "^6.0.1",
    "fastify-static": "^4.2.4",
    "fs-extra": "^10.0.0",
    "get-port": "^5.1.1",
    "hash.js": "^1.1.7",
    "hasha": "^5.2.2",
    "image-size": "^0.9.3",
    "loader-utils": "^2.0.0",
    "metro-inspector-proxy": "^0.64.0",
    "open": "^8.0.2",
    "open-editor": "^3.0.0",
    "ora": "^5.3.0",
    "pretty-format": "^26.6.2",
    "schema-utils": "^3.0.0",
    "source-map": "^0.7.3",
    "split2": "^3.2.2",
    "tapable": "^2.1.1",
    "webpack-dev-middleware": "^4.1.0",
    "ws": "^7.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@callstack/eslint-config": "^11.0.0",
    "@react-native-community/cli": "5.0.1-alpha.1",
    "@react-native-community/cli-types": "5.0.1-alpha.1",
    "@release-it/conventional-changelog": "^3.0.1",
    "@types/babel__code-frame": "^7.0.2",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.28",
    "@types/react-dom": "^17.0.7",
    "@types/split2": "^2.1.6",
    "@types/ws": "^7.4.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "del-cli": "^3.0.1",
    "eslint": "^7.30.0",
    "jest": "^26.6.3",
    "react-native": "^0.64.1",
    "release-it": "^14.10.0",
    "terser-webpack-plugin": "^5.1.3",
    "typedoc": "^0.21.8",
    "typedoc-plugin-markdown": "^3.10.4",
    "typescript": "^4.1.5",
    "webpack": "^5.50.0"
  }
}
